mode1:	move.l	d7,d1
	swap	d7		* d7 = 00000000_000000pp
	ror.w	#2,d7		* d7 = pp000000_00000000
	or.w	d7,d1		* d1 = PC (R800)
	subq.w	#2,sp_(a3)	* PUSH	
	move.w	sp_(a3),d0
	jsr	ms_write_mem_16

