w_port_A8:
	move.b	d1,slot_reg

	lea.l	slot_0_0,a0
	lea.l	base2,a2

	move.b	d1,d0
	andi.w	#%00000011,d0			* ページ０の切り替え
	move.b	exslot_reg_0-base(a3,d0.w),d2	* 拡張スロット選択レジスタの値を読む
	andi.w	#%00000011,d2			* d2 = ページ０の拡張スロット番号
	lsl.w	#4,d2
	lsl.w	#6,d0
	add.w	d2,d0
