	move.l	rd16_sub_add_0-base2(a2,d0.w*4),rd16_sub_add_p3-base2(a2)

*	ページ０は表に出てるか？
	move.b	slot_reg,d0
	andi.w	#%00000000_00000011,d0
	move.w	(sp),d2
