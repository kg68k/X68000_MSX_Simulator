none_DD_FD_CB:					* おかしな命令を実行しました
	move.w	d0,d1
	lsr.w	#4,d0
	lea.l	HEX,a0
	move.b	(a0,d0.w),number_ddfdcb+0
	andi.w	#$0f,d1
	move.b	(a0,d1.w),number_ddfdcb+1

	pea.l	errms_ddfdcb
	DOS	__PRINT
	lea.l	4(sp),sp
	move.b	#$ff,d0
	rts

errms_ddfdcb:
	.dc.b	'おかしな命令を実行しました。0xdd or 0xfd,0xcb,0x'
number_ddfdcb:
	.dc.b	'00'
	.dc.b	0x0d,0x0a,0x00

	.even


; d0 には Idx + d がセットされて呼ばれる
RLC_lIdx_dl:
	move.w	d0,-(sp)		* Idx + d を退避
	jsr	ms_read_mem_8
	move.b	d0,target_8(a3)
	RXX_	1, 1
	move.b	d0,d1
	move.w	(sp)+,d0
	jsr	ms_write_mem_8
	rts

RRC_lIdx_dl:
	move.w	d0,-(sp)		* Idx + d を退避
	jsr	ms_read_mem_8
	move.b	d0,target_8(a3)
	RXX_	0, 1
	move.b	d0,d1
	move.w	(sp)+,d0
	jsr	ms_write_mem_8
	rts

RL_lIdx_dl:
	move.w	d0,-(sp)		* Idx + d を退避
	jsr	ms_read_mem_8
	move.b	d0,target_8(a3)
	RXX_	1, 0
	move.b	d0,d1
	move.w	(sp)+,d0
	jsr	ms_write_mem_8
	rts

RR_lIdx_dl:
	move.w	d0,-(sp)		* Idx + d を退避
